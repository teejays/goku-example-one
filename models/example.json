    "services": [
        {
            "name": "Users",
            "options": {
                "frontend_options": {
                    "default_icon": "TeamOutlined"
                }
            },
            "entities": [
                {
                    "name": "User",
                    "fields": [
                        {
                            "name": "Name",
                            "data_type": {
                                "name": "PersonName",
                                "kind": "Nested",
                                "reference_namespace": {
                                    "app": "ExampleApp",
                                    "types": [
                                        "PersonName"
                                    ]
                                }
                            },
                            "options": {
                                "nested_options": {
                                    "expand_fields": false
                                }
                            }
                        },
                        {
                            "name": "Email",
                            "data_type": "Email",
                            "options": {
                                "default_database_value": "no@email.com",
                                "nested_options": {
                                    "expand_fields": false
                                }
                            }
                        },
                        {
                            "name": "PhoneNumber",
                            "data_type": {
                                "name": "PhoneNumber",
                                "kind": "Nested",
                                "reference_namespace": {
                                    "app": "ExampleApp",
                                    "types": [
                                        "PhoneNumber"
                                    ]
                                }
                            },
                            "options": {
                                "is_optional": true,
                                "nested_options": {}
                            }
                        },
                        {
                            "name": "PasswordHash",
                            "data_type": "String",
                            "options": {
                                "exclude_from_gateway_input": true
                            }
                        }
                    ]
                }
            ],
            "types": [
                {
                    "name": "AuthenticateRequest",
                    "fields": [
                        {
                            "name": "Email",
                            "data_type": "String"
                        },
                        {
                            "name": "Password",
                            "data_type": "String"
                        }
                    ],
                    "options": {
                        "kind": "WireRequest"
                    }
                },
                {
                    "name": "AuthenticateResponse",
                    "fields": [
                        {
                            "name": "Token",
                            "data_type": "String"
                        }
                    ],
                    "options": {
                        "kind": "WireResponse"
                    }
                },
                {
                    "name": "RegisterUserRequest",
                    "fields": [
                        {
                            "name": "Email",
                            "data_type": "Email"
                        },
                        {
                            "name": "Name",
                            "data_type": {
                                "name": "PersonName",
                                "kind": "Nested",
                                "reference_namespace": {
                                    "app": "ExampleApp",
                                    "types": [
                                        "PersonName"
                                    ]
                                }
                            }
                        },
                        {
                            "name": "PhoneNumber",
                            "data_type": {
                                "name": "PhoneNumber",
                                "kind": "Nested",
                                "reference_namespace": {
                                    "app": "ExampleApp",
                                    "types": [
                                        "PhoneNumber"
                                    ]
                                }
                            }
                        },
                        {
                            "name": "Password",
                            "data_type": "String"
                        }
                    ],
                    "options": {
                        "kind": "WireRequest"
                    }
                }
            ],
            "methods": [
                {
                    "name": "AuthenticateUser",
                    "description": "Handle authentication of users",
                    "argument_type": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "types": [
                            "AuthenticateRequest"
                        ]
                    },
                    "return_type": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "types": [
                            "AuthenticateResponse"
                        ]
                    },
                    "options": {
                        "type": "Query"
                    }
                },
                {
                    "name": "RegisterUser",
                    "description": "Create a new user",
                    "argument_type": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "types": [
                            "RegisterUserRequest"
                        ]
                    },
                    "return_type": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "types": [
                            "AuthenticateResponse"
                        ]
                    },
                    "options": {
                        "type": "Mutation"
                    }
                }
            ],
            "apis": [
                {
                    "path": "users/register",
                    "http_method": "POST",
                    "use_body_for_request": true,
                    "success_status": 200,
                    "method_reference_namespace": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "method": "RegisterUser"
                    }
                },
                {
                    "path": "users/authenticate",
                    "http_method": "POST",
                    "use_body_for_request": true,
                    "success_status": 200,
                    "method_reference_namespace": {
                        "app": "ExampleApp",
                        "service": "Users",
                        "method": "AuthenticateUser"
                    }
                }
            ]
        },
